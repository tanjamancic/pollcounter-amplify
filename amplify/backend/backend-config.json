{
	"storage": {
		"pollCounterDDB": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"pollCounterFunction": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "pollCounterDDB",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"pollCounterAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "pollCounterFunction",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}